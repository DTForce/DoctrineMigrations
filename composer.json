{
	"name": "dtforce/fornettey-doctrine-migrations",
	"description": "Implementation of Doctrine Migrations for Nette",
	"license": "MIT",
	"authors": [
		{ "name": "Tomáš Votruba", "homepage": "http://tomasvotruba.cz" },
		{ "name": "DTForce - Jan Mares", "homepage": "http://www.dtforce.com" }
	],
	"require": {
		"php": ">=5.6",
		"doctrine/migrations": "~1.0",
		"nette/di": "~2.3",
		"symfony/console": "~2.7|~3.0"
	},
	"require-dev": {
		"dtforce/nette-console": "dev-master",
		"dtforce/nette-doctrine": "dev-master",
		"nette/bootstrap": "~2.3",
		"tracy/tracy": "~2.3"
	},
	"autoload": {
		"psr-4": {
			"DTForce\\DoctrineMigrations\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"DTForce\\DoctrineMigrations\\Tests\\": "tests"
		}
	}
}
